<!DOCTYPE html>
<html>
<head>
  <title>CvBuilder</title>
  <%= stylesheet_link_tag    "application", media: "all", "data-turbolinks-track" => true %>
  <%= javascript_include_tag "students/application", "data-turbolinks-track" => true %>
  <%= csrf_meta_tags %>
  <%= yield :extra_js %>
</head>
<body>
 <div id="container" class="container">
      <header>
        <%= render 'layouts/student_navigation' %>
        <%= render 'layouts/messages' %>
      </header>
      <div id="main" role="main">
        <%= yield %>
      </div>
    
    </div> <!--! end of #container -->
</body>
</html>