<h1>Login</h1>
 
<%= form_tag student_create_login_path, method: :post do %>
  <div class="field">
    <%= label_tag :"Register No" %><br />
    <%= text_field_tag :register_no %>
  </div>
  <div class="field">
    <%= label_tag :password %><br />
    <%= password_field_tag :password %>
  </div>
  <div class="actions">
    <%= submit_tag 'Login' %>
  </div>
<% end %>